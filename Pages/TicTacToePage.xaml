<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BoardGames.Pages.TicTacToePage"
			 xmlns:viewmodel="clr-namespace:BoardGames.ViewModels"
			 x:DataType="viewmodel:TicTacToeViewModelcs"
				 xmlns:models="clr-namespace:BoardGames.Models"
             Title="Tic Tac Toe">
	<VerticalStackLayout>
		<Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

		<CollectionView>
			<CollectionView.ItemsLayout>
				<GridItemsLayout Span="3" Orientation="Vertical"/>
			</CollectionView.ItemsLayout>
			<CollectionView.ItemTemplate>
				<DataTemplate x:DataType="{Binding  }" >
					<Frame>
						<Label Text="Bi"/>
					</Frame>
				</DataTemplate>
			</CollectionView.ItemTemplate>
		</CollectionView>
	</VerticalStackLayout>
</ContentPage>